{
  "title": "GE: US International keyboard layout for German users",
  "rules": [
    {
      "description": "Swap z with y for better compatibility to German layout",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "z"
          },
          "to": {
            "key_code": "y"
          }
        }
      ]
    },
    {
      "description": "Swap Z with Y for better compatibility to German layout (Shift)",
      "manipulators": [
        {
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": [
                "left_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "y",
              "modifiers": [
                "left_shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Swap y with z for better compatibility to German layout",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "y"
          },
          "to": {
            "key_code": "z"
